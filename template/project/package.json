{
  "name": "leviate-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "dev:clean": "vite --force",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "serve": "vite preview",
    "deploy-staging": "npm install && npm run build -- --minify false && node scripts/deploy-to-staging.js",
    "make-migration": "node scripts/make-migration.js",
    "generate-migration-index": "node scripts/generate-migration-index.js",
    "test": "jest --coverage=true --verbose",
    "cy:run": "cypress run --env port=8081",
    "cy:open": "cypress open --env port=8080",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 cy:open",
    "test:e2e": "npm run build:test && start-server-and-test serve http://localhost:8081 cy:run",
    "start:test:cypress": "start-server-and-test serve http://localhost:8080"
  },
  "dependencies": {
    "@crhio/leviate": "^5.0.3",
    "@crhio/normie": "^1.0.0",
    "@headlessui/vue": "^1.6.7",
    "@heroicons/vue": "^2.0.18",
    "axios": "^0.21.1",
    "core-js": "^3.16.0",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.14",
    "uuid": "^8.3.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.12",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@crhio/leviat-tailwind-configuration": "^1.2.1",
    "@headlessui/tailwindcss": "^0.1.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "autoprefixer": "^10.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cypress": "^12.17.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.6.0",
    "express": "^4.17.1",
    "jiti": "^2.4.2",
    "postcss": "^8.3.6",
    "s3-folder-upload": "^2.3.5",
    "sass": "1.32",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.4.17",
    "vite": "^3.0.9",
    "vite-svg-loader": "^3.3.0"
  }
}
